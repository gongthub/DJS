<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!--<add name="AssemblySrc" connectionString="HelloWord.dll"/>-->
    <!--DLL 文件夹配置-->
    <add key="AssemblySrc" value="Stores" />
    <!--SDK接口实现类型-->
    <add key="SDKSERVICETYPE" value="Service" />
    <!--数据持久类型 Redis XML -->
    <add key="DALTYPE" value="XML" />
    <!--Redis 配置文件路径-->
    <add key="RedisConfig" value="RedisConfig.xml" />
    <!--xml存储文件路径-->
    <add key="XmlDBConfig" value="XmlDB.xml" />
    <!--SDK配置文件路径-->
    <add key="SDKCONFIG" value="SDKConfig.xml" />
    <!--Redis 服务端地址-->
    <add key="redisServer" value="127.0.0.1" />
    <!--Redis 服务端端口-->
    <add key="redisServerPort" value="6379" />
    <!--Redis 客户端 License-->
    <add key="servicestack:license" value="1001-" />
    <!--Cron 路径 文件绝对路径-->
    <add key="CronUrl" value="Cron\index.htm" />
    <!--Quartz 实现接口名称-->
    <add key="IQuartz" value="IJob" />
    <!--新浪调用接口地址-->
    <add key="SinaUrl" value="http://hq.sinajs.cn/" />
    <!--新浪调用接口 参数-->
    <add key="SinaPram" value="list=sz002626" />
    <!--数据回写文件文件夹-->
    <add key="DataPath" value="Data" />
    <!--数据回写文件名称格式 必须以时间格式-->
    <add key="DataName" value="yyyy-MM-dd" />
    <!--日志类型 Redis File NLog-->
    <add key="LogFileType" value="NLog" />
    <!--日志文件路径-->
    <add key="LogUrl" value="Log" />
    <!--NLog日志文件路径-->
    <add key="NLogUrl" value="Nlogs" />
    <!--日志文件名称规则 必须以时间格式3-->
    <add key="LogName" value="yyyy-MM-dd" />
    <!--日志显示条数-->
    <add key="LogShowNum" value="100" />
    <!--日志类型 Redis File NLog-->
    <add key="JobFileSrc" value="JobFiles" />
    <!--程序路径-->
    <add key="APPURL" value="DJS.WinApp.exe" />
    <!--是否自启动-->
    <add key="APPAUTO" value="false" />
    <!--错误邮件发送人-->
    <add key="ERRORSENDUSER" value="erp@vlinker.com.cn" />
    <!--错误邮件发送密码-->
    <add key="ERRORSENDPWD" value="Vlinker123" />
    <!--错误邮件发送服务器-->
    <add key="ERRORSENDSMTP" value="smtp.exmail.qq.com" />
    <!--错误邮件发送收件人-->
    <add key="ERRORTOUSER" value="gongtao@vlinker.com.cn" />
    <!--错误邮件发送抄送人-->
    <add key="ERRORCCUSER" value="" />
    <!--错误邮件发送密送人-->
    <add key="ERRORBCUSER" value="" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>